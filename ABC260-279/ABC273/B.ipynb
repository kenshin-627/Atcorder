   "source": [
    "from decimal import Decimal, ROUND_HALF_UP\n",
    "X_K = input()\n",
    "X = float(X_K.split()[0])\n",
    "K = int(X_K.split()[1])\n",
    "\n",
    "for _ in range(K):\n",
    "    X /= 10\n",
    "    X = Decimal(str(X)).quantize(Decimal('0'), rounding=ROUND_HALF_UP)\n",
    "\n",
    "X = X * 10 ** K\n",
    "print(X)"
   ]
