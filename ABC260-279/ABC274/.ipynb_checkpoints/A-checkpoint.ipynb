    "from decimal import Decimal, ROUND_HALF_UP\n",
    "def main():\n",
    "    A_B = list(map(int, input().split()))\n",
    "    A = A_B[0]\n",
    "    B = A_B[1]\n",
    "    print(Decimal(str(B/A)).quantize(Decimal('0.001'), rounding=ROUND_HALF_UP))\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
