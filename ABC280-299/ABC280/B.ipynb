    "def main():\n",
    "    N = int(input())\n",
    "    SN = [0] + [*map(int, input().split())]\n",
    "    for i in range(N):\n",
    "        SN[i] = SN[i+1] - SN[i]\n",
    "    SN.pop(-1)\n",
    "    print(\" \".join(map(str, SN)))\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
