    "def main():\n",
    "    T = int(input())\n",
    "    for _ in range(T):\n",
    "        N = int(input())\n",
    "        AN = [*map(int, input().split())]\n",
    "        count = 0\n",
    "        for A in AN:\n",
    "            if A % 2 == 1:\n",
    "                count += 1\n",
    "        print(count)\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
