    "def main():\n",
    "    N = int(input())\n",
    "    count = 0\n",
    "    for _ in range(N):\n",
    "        S = input()\n",
    "        if S == \"For\":\n",
    "            count += 1\n",
    "    if N // 2 < count:\n",
    "        print(\"Yes\")\n",
    "    else:\n",
    "        print(\"No\")\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
