    "def main():\n",
    "    S = input()\n",
    "    ans = str()\n",
    "    tmp = []\n",
    "    for i, s in enumerate(S):\n",
    "        if i % 2 == 0:\n",
    "            tmp = [s]\n",
    "        else:\n",
    "            tmp.append(s)\n",
    "            ans += \"\".join(reversed(tmp))\n",
    "    print(ans)\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
