    "def main():\n",
    "    N = int(input())\n",
    "    AN = [*map(int, input().split())]\n",
    "    ans = []\n",
    "    for a in AN:\n",
    "        if a % 2 == 0:\n",
    "            ans.append(str(a))\n",
    "    print(\" \".join(ans))\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
