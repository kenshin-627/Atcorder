    "def main():\n",
    "    N,D = map(int, input().split())\n",
    "    TN = [*map(int, input().split())]\n",
    "    for i in range(1, N):\n",
    "        if TN[i]-TN[i-1] <= D:\n",
    "            print(TN[i])\n",
    "            return\n",
    "    print(-1)\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
