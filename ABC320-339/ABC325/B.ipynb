   "source": [
    "def main():\n",
    "    N = int(input())\n",
    "    ans = [0]*24\n",
    "    for _ in range(N):\n",
    "        W,X = list(map(int, input().split()))\n",
    "        for i in range(24):\n",
    "            if 9 <= (X+i) % 24 < 18:\n",
    "                ans[i%24] += W\n",
    "    print(max(ans))\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
