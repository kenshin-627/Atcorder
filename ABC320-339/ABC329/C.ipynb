    "from itertools import groupby\n",
    "from collections import defaultdict\n",
    "\n",
    "def main():\n",
    "    N = int(input())\n",
    "    S = input()\n",
    "    d = defaultdict(int)\n",
    "    for k,v in groupby(S):\n",
    "        d[k] = max(d[k],len(list(v)))\n",
    "    print(sum(d.values()))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
