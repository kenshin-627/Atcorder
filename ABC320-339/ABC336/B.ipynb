    "def main():\n",
    "    N = int(input())\n",
    "    N_bin = bin(N)[2:]\n",
    "    ans = 0\n",
    "    for n in reversed(N_bin):\n",
    "        if n == \"0\":\n",
    "            ans += 1\n",
    "        else:\n",
    "            break\n",
    "    print(ans)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
