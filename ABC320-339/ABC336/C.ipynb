    "def main():\n",
    "    N = int(input()) - 1\n",
    "    if N == 0:\n",
    "        print(0)\n",
    "        exit()\n",
    "    ans = []\n",
    "    while N != 0:\n",
    "        ans.append(N % 5)\n",
    "        N //= 5\n",
    "    ans.reverse()\n",
    "    ans = [str(i*2) for i in ans]\n",
    "    print(\"\".join(ans))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
