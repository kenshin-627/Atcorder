    "def main():\n",
    "    N = int(input())\n",
    "    A = list(map(int, input().split()))\n",
    "    tmp = 0\n",
    "    min_ = 0\n",
    "    for i in range(N):\n",
    "        tmp += A[i]\n",
    "        min_ = min(min_, tmp)\n",
    "    print(tmp - min_)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
